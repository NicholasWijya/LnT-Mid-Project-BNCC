
public class PT_ChipiChapa {
	String kode;
	String nama;
	String jenisKelamin;
	String jabatan;
	Integer gaji;

	public PT_ChipiChapa(String kode, String nama, String jenisKelamin, String jabatan, Integer gaji) {
		super();
		this.kode = kode;
		this.nama = nama;
		this.jenisKelamin = jenisKelamin;
		this.jabatan = jabatan;
		this.gaji = gaji;
	}

	 public void setGaji(int newGaji) {
	        this.gaji = newGaji;
	    }

	    public int getGaji() {
	        return this.gaji;
	    }

	    public String getKode() {
	        return this.kode;
	    }

}
